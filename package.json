{
  "name": "node_shop",
  "version": "1.0.0",
  "description": "Microservices built in Node.js + Express to interact with Amazon Simple Queue Service(SQS).",
  "main": "index.js",
  "scripts": {
    "start-orders-svc": "node ./orderssvc/index.js 8081",
    "start-emails-svc": "node ./emailssvc/index.js 8082",
    "start": "npm-run-all -p -r start-orders-svc start-emails-svc"
  },
  "keywords": [
    "node",
    "nodejs",
    "express",
    "bodyparser",
    "amazon",
    "sqs",
    "amazonsqs",
    "microservices"
  ],
  "author": "robley <https://ro6ley.github.io>",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.570.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5"
  }
}
